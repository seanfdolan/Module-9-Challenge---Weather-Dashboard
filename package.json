  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.3",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "sequelize": "^6.37.5"
  }
